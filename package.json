{
  "name": "snapkey",
  "version": "2.0.0",
  "description": "A lightweight, open-source tool that prevents simultaneous movement key conflicts",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "electron-rebuild -f -w uiohook-napi",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cafali/SnapKey.git"
  },
  "keywords": [
    "electron",
    "gaming",
    "keyboard",
    "input"
  ],
  "author": "cafali",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cafali/SnapKey/issues"
  },
  "homepage": "https://github.com/cafali/SnapKey#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/rebuild": "^3.7.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.0",
    "electron": "^39.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "electron-store": "^11.0.2",
    "koffi": "^2.14.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uiohook-napi": "^1.5.4",
    "zod": "^4.1.12"
  }
}
